<tr>
  <td><%= req.start_date %></td>
  <td><%= req.end_date %></td>
  <td>
    <% if req.pending? %>
        <% unless current_user.nil? || @cat.owner != current_user %>
        <%= button_to "Approve", approve_cat_rental_request_url(req) %>
        <%= button_to "Deny", deny_cat_rental_request_url(req) %>
        <% end %>
    <% else %>
      <%= req.status %>
    <% end %>
  </td>
  <td><%= req.user.username %></td>
</tr>
